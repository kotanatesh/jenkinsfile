node('master') 
{
 stage('checkout')
 {
    git ' https://github.com/selenium-saikrishna/maven.git'
 }
 stage('build') 
 {
    sh 'mvn package'
}
stage('deploy')
{
    sh ' scp 
/home/ubuntu/workspace/workspace/pipeline/webapp/target/webapp.war 
ubuntu@52.66.245.54:/var/lib/tomcat7/webapps/qaenv.war'

}
stage('testing')
{
git 'https://github.com/selenium-saikrishna/FunctionalTesting.git'
sh 'java -jar /home/ubuntu/workspace/workspace/pipeline/testing.jar'

}
stage('prod_del') 
{
      sh ' scp 
/home/ubuntu/workspace/workspace/pipeline/webapp/target/webapp.war 
ubuntu@13.232.102.189:/var/lib/tomcat7/webapps/prodenv.war'

}


   
}
